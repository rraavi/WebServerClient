#ifndef HEADER_H_
#define HEADER_H_

const int MAXSIZE=20000;
const int MSS=1460;
using namespace std;

struct header
{
	int seq_no=0;
	int ack_no=0;
	bool ackflag;
	bool finflag;
	unsigned short length;
	char data[MSS];
};

#endif /* HEADER_H_ */
